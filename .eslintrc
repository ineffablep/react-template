{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "require": true,
    "module": true,
    "global": true,
    "window": true,
    "describe": true,
    "it": true,
    "beforeAll": true,
    "beforeEach": true,
    "afterEach": true,
    "afterAll": true,
    "expect": true,
    "__dirname": true,
    "spyOn": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      1,
      "tab",
      {
        "SwitchCase": 1
      }
    ],
    "brace-style": [
      1,
      "1tbs"
    ],
    "comma-style": [
      1,
      "last"
    ],
    "default-case": 1,
    "func-style": [
      1,
      "declaration"
    ],
    "no-floating-decimal": 1,
    "no-nested-ternary": 1,
    "no-undefined": 1,
    "radix": 2,
    "space-before-function-paren": [
      1,
      "never"
    ],
    "space-before-blocks": 1,
    "spaced-comment": [
      1,
      "always",
      {
        "exceptions": [
          "-"
        ]
      }
    ],
    "valid-jsdoc": [
      1,
      {
        "requireReturn": false,
        "prefer": {
          "return": "returns"
        }
      }
    ],
    "wrap-iife": 1,
    "global-strict": 0,
    "no-alert": 1,
    "camelcase": 1,
    "eqeqeq": [
      1,
      "allow-null"
    ],
    "no-empty": 1,
    "no-underscore-dangle": 0,
    "no-obj-calls": 1,
    "new-cap": 1,
    "no-shadow": 1,
    "no-invalid-regexp": 1,
    "comma-dangle": [
      1,
      "never"
    ],
    "no-undef": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-new": 1,
    "no-extra-semi": 1,
    "no-debugger": 1,
    "no-caller": 1,
    "semi": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-unreachable": 1,
    "eol-last": 1,
    "curly": 1,
    "dot-notation": 1,
    "yoda": 1,
    "no-trailing-spaces": 1,
    "no-multi-spaces": 1,
    "no-mixed-spaces-and-tabs": [
      1,
      "smart-tabs"
    ],
    "react/jsx-curly-spacing": [
      1,
      "never"
    ],
    "react/jsx-max-props-per-line": [
      1,
      {
        "maximum": 4
      }
    ],
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-direct-mutation-state": 1,
    "react/no-multi-comp": 1,
    "react/no-unknown-property": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/self-closing-comp": 1,
    "react/wrap-multilines": 1,
    "react/sort-comp": [2, {
      "order": [
        "lifecycle",
        "/^on.+$/",
        "/^render.+$/",
        "render",
        "everything-else"
      ]
    }]
  },
  "ecmaFeatures": {
    "jsx": true,
    "destructuring": true,
    "spread": true,
    "blockBindings": true,
    "modules": true
  }
}
